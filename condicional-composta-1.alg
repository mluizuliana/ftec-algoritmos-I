algoritmo "numeros_iguais"
var
   n1, n2, n3: inteiro
inicio

      escreval("Digite três números: ")
      leia(n1)
      leia(n2)
      leia(n3)
      se (n1 = n2) e (n2 = n3) entao
         escreval("Igual")
      senao
         escreval("Diferente")
      fimse
      
fimalgoritmo
